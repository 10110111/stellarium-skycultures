msgstr "Дух поларног медведа"

# Name for HIP 11767
msgid "Never Moves"
msgstr "Непокретан"

# Name for HIP 37279
msgid "Name of a Murdered Man"
msgstr "Име убијеног човека"

# Name for HIP 32349
msgid "Flickering"
msgstr "Треперење"

# Name for HIP 26241
msgid "Nephews"
msgstr "Нећаци"

# Name for HIP 69673
msgid "The Old Man"
msgstr "Старац"

# Name for HIP 67927
msgid "The Little Orphan Boy"
msgstr "Сироче"

# Name for NAME Orion Nebula
msgid "Nephews or Nieces"
msgstr ""
